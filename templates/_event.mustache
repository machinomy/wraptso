  export interface {{this.name}} {
  {{#if inputs}}
    {{#each inputs}}
    {{name}}: {{#inputType type}}{{/inputType}}
    {{/each}}
  {{/if}}
  }

  export function is{{this.name}}Event (something: any): something is truffle.TransactionEvent<{{this.name}}> {
    return something.event === '{{this.name}}'
  }
